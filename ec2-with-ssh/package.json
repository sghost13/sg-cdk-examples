{
  "name": "ec2-with-ssh",
  "version": "0.1.0",
  "bin": {
    "ec2-with-ssh": "bin/ec2-with-ssh.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "MY_IPV4=$(curl -s https://api.ipify.org) && cdk deploy -c IPV4=$MY_IPV4/32",
    "destroy": "MY_IPV4=$(curl -s https://api.ipify.org) && cdk destroy -c IPV4=$MY_IPV4/32"
  },
  "devDependencies": {
    "@types/node": "22.7.9",
    "aws-cdk": "2.1007.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.188.0",
    "constructs": "^10.0.0"
  }
}
